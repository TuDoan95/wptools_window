# WordPress Configuration
WP_URL=https://example.com
WP_USERNAME=your_username
WP_APP_PASSWORD=your_app_password
WP_USE_YOAST_SEO=true
WP_DEFAULT_CATEGORY=General
WP_API_RATE_LIMIT=30

# Gemini AI Configuration
GEMINI_API_KEY1=your_gemini_api_key
GEMINI_API_KEY2=your_backup_gemini_api_key
GEMINI_MODEL_NAME=gemini-pro
GEMINI_MAX_KEY_ERRORS=5
GEMINI_KEY_ERROR_COOLDOWN=300
GEMINI_RATE_LIMIT=5

# Media Configuration
IMAGE_MAX_COUNT=5
IMAGE_RATE_LIMIT=2
VIDEO_MAX_COUNT=1
VIDEO_RATE_LIMIT=2

# Selenium Configuration
SELENIUM_HEADLESS=true
SELENIUM_TIMEOUT=10
SELENIUM_MAX_SCROLLS=3

# Cache Configuration
CACHE_TTL=604800
CACHE_MAX_ITEMS=1000

# Log Configuration
LOG_LEVEL=INFO
LOG_RETENTION_DAYS=7